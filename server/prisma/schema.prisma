generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model favorites {
  id            Int       @default(autoincrement()) @id
  movieId       Int?
  releaseDate   String?   @db.VarChar
  posterImage   String?   @db.VarChar
  description   String?   @db.VarChar
  averageRating String?   @db.VarChar
}